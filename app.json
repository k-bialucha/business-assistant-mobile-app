{
  "expo": {
    "name": "Bussiness Assistant Mobile App",
    "slug": "business-assistant-mobile-app",
    "entryPoint": "./src/index.ts",
    "privacy": "public",
    "sdkVersion": "38.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "0.0.2",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  }
}
